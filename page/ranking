	$('.hover-column-first').on("mouseenter", function(){
		$('.hover-column-first').css('background-color','white');
		$(this).css('background-color','#ededed');
	});

	$('.hover-column-second').on("mouseenter", function(){
		$('.hover-column-second').css('background-color','white');
		$(this).css('background-color','#ededed');
	});


	$('.dropdown-menu').on("mouseleave", function(){
		$('.hover-column-first, .hover-column-second').css('background-color','white');
